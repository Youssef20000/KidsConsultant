ul#mobile-demo.sidenav
    li
        div.user-view
            div.background
                img(src="data:image/jpeg;base64,")
            if (auth)
                a(href="#user")
                    img.circle(src=auth.photo || "/img/icons/man.png")
                a(href="#name")
                    span(class="white-text name")=auth.name
                a(href="#email")
                    span.white-text.email=auth.email
            else
                div.container.text-center
                    a.btn.bg-transparent.white-text(href="/login" style="color:white !important") Login
                    a.btn.bg-transparent.white-text(href="/register" style="color:white !important") Register
    li
        div.divider
    li
        a(href="/")
            span.icofont.icofont-ui-home.mr-1
            span="Home"
    if (isAuthenticated)
        li
            a(href="/profile")
                span.icofont.icofont-ui-user.mr-1
                sapn Profile
        li
            a(href="/kids")
                span.icofont.mr-1.icofont-group
                sapn Your Kids
        li
            a(href="/chats")
                span.icofont.mr-1.icofont-ui-chat
                sapn Chats
        li
            a(href="/settings")
                span.icofont.mr-1.icofont-gears
                sapn Account Settings
        li
            a(href="/logout")
                span.icofont.mr-1.icofont-sign-out
                span="Log Out"
    li
        div.divider
    li
        a(href="/policy")
            span.icofont.icofont-paper.mr-1
            sapn Our Policy
